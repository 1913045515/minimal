/**post**/

.post-card, .post-feed {
  display: -ms-flexbox;
  display: flex;
}

.post-card {
  -ms-flex: 1 1 300px;
  /* flex: 1 1 300px; */
  -ms-flex-direction: column;
  flex-direction: column;
  overflow: hidden;
  margin: 0 20px 20px;
  background: #fff 50%;
  background-size: cover;
  border-radius: 5px;
  box-shadow: 2rpx 4rpx 40rpx #c2c2c2;
  transition: all 0.5s ease;
  width: 90%;
  opacity: 0;
}

.post-card-image-link {
  /* position: relative; */
  display: block;
  overflow: hidden;
  border-radius: 5px 5px 0 0;
}

.post-card-content {
  -ms-flex-positive: 1;
  flex-grow: 1;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-direction: column;
  flex-direction: column;
  -ms-flex-pack: justify;
  justify-content: space-between;
  width: 95%;
  height: 100%;
  /* border: 1px solid red; */
  margin: 5px auto;
}

